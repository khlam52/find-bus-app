<shield>
    <config>
        <!-- 
            blockDebugger, 
            checkDebugger,
            exitOnDebugger, 
            checkRepackaging,
            exitOnRepackaging,
         - On release builds of Shield this option is always enabled and cannot be disabled. 
         -->
